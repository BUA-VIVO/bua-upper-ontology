Declaration(AnnotationProperty(vitro:selectFromExistingAnnot))
Declaration(AnnotationProperty(:geritId))
############################
#   Annotation Properties
############################

# Annotation Property: dcterms:description (description)

AnnotationAssertion(rdfs:label dcterms:description "description")

# Annotation Property: dcterms:license (license)

AnnotationAssertion(rdfs:label dcterms:license "license")

# Annotation Property: dcterms:title (title)

AnnotationAssertion(rdfs:label dcterms:title "title")

# Annotation Property: :geritId (Gerit Id)

AnnotationAssertion(rdfs:label :geritId "Gerit Id")



############################
#   Classes
############################

# Class: :BUA_0000000 (Arbeitsbereich)

AnnotationAssertion(obo:IAO_0000115 :BUA_0000000 "Part of Academic Apartment"@en)
AnnotationAssertion(vitro:hiddenFromDisplayBelowRoleLevelAnnot :BUA_0000000 <http://vitro.mannlib.cornell.edu/ns/vitro/role#public>)
AnnotationAssertion(vitro:hiddenFromPublishBelowRoleLevelAnnot :BUA_0000000 <http://vitro.mannlib.cornell.edu/ns/vitro/role#public>)
AnnotationAssertion(vitro:inClassGroup :BUA_0000000 <http://vivoweb.org/ontology#vitroClassGrouporganizations>)
AnnotationAssertion(vitro:prohibitedFromUpdateBelowRoleLevelAnnot :BUA_0000000 <http://vitro.mannlib.cornell.edu/ns/vitro/role#public>)
AnnotationAssertion(rdfs:label :BUA_0000000 "Arbeitsbereich"@de)
