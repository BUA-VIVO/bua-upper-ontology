Ontology(<http://vivo.berlin-university-alliance.de/ontology/core/v1/bua/bua.owl>
Annotation(dcterms:description "This is the Upper intology for the Berlin University Alliance"@en-us)
Annotation(dcterms:license <http://creativecommons.org/publicdomain/zero/1.0/>)
Annotation(dcterms:title "VIVO BUA Upper ontology")
Annotation(rdfs:label "Berlin University Alliance Obere Ontologie"@de-de)
Annotation(rdfs:label "Berlin University Alliance Upper Ontology"@en-us)

Declaration(Class(:BUA_0000000))
Declaration(Class(<http://vivoweb.org/ontology/core#AcademicUnit>))
Declaration(AnnotationProperty(obo:IAO_0000115))
Declaration(AnnotationProperty(dcterms:description))
Declaration(AnnotationProperty(dcterms:license))
Declaration(AnnotationProperty(dcterms:title))
Declaration(AnnotationProperty(vitro:hiddenFromDisplayBelowRoleLevelAnnot))
Declaration(AnnotationProperty(vitro:hiddenFromPublishBelowRoleLevelAnnot))
Declaration(AnnotationProperty(vitro:inClassGroup))
Declaration(AnnotationProperty(vitro:inPropertyGroupAnnot))
Declaration(AnnotationProperty(vitro:ontologyPrefixAnnot))
